else
   status_haproxy="${RED}Offline ${NC}"
fi

# STATUS NOOBZVPNS
if [[ $noobz == "running" ]]; then 
   noobz=" ${GREEN}Online ${NC}"
else
   noobz="${RED}Offline ${NC}"
fi
clear
echo -e "$COLOR1â•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${NC}"
echo -e "$COLOR1â”‚ ${NC}${COLBG1}              ${WH}â‡± SERVICE INFORMATION â‡²            ${NC}${COLOR1} â”‚$NC"
